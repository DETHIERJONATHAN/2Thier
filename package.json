    "db:restore:json": "tsx scripts/restore-db-json.ts",
    "db:restore:sql": "node restore-database.js",
    "build": "vite build",
    "build:client": "tsc -p tsconfig.build.json && vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "start": "NODE_ENV=production node dist/api-server.js",
    "prod": "npm run build && npm run start",
    "test:ai": "tsx scripts/test-ai-calendar.ts",
    "test:ai:extended": "tsx scripts/test-ai-extended.ts",
    "test:ai:guard": "AI_MODE=force-live tsx scripts/test-ai-guard.ts",
