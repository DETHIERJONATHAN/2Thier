    "test:kwh:matrix": "node scripts/test-kwh-condition-matrix.cjs",
    "test:kwh:auto": "node scripts/run-kwh-tests-auto.cjs",
    "dev:token": "node scripts/generate-dev-token.cjs --email=jonathan.dethier@2thier.be",
    "get:token": "node scripts/get-token.cjs --export",
    "db:create:client-form": "tsx scripts/create-client-info-form.ts",
    "db:seed:field-types": "tsx scripts/seed-field-types.ts",
    "db:map:client-dbfields": "tsx scripts/map-dbfields-client.ts",
    "devis1minute:activate": "node activate-devis1minute-modules.js",
    "devis1minute:test": "node test-devis1minute-system.js",
    "devis1minute:count": "node count-devis1minute-modules.js",
    "perf:lighthouse": "npx lighthouse http://localhost:5173/devis1minute --output=json --only-categories=performance",
    "perf:lighthouse:full": "npx lighthouse http://localhost:5173/devis1minute --output=html --output-path=lighthouse-report.html",
    "perf:analyze": "npm run build:analyze && open dist/stats.html"
