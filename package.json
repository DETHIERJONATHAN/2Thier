{
  "name": "crm2thier-vite",
  "private": true,
  "version": "0.1.1",
  "engines": {
    "node": "20"
  },
  "type": "module",
  "scripts": {
    "gcp-build": "npm run build",
    "prisma:generate": "prisma generate",
    "dev:server": "tsx watch --clear-screen=false --env-file=.env src/api-server-clean.ts",
    "dev:client": "vite",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "auto:commit": "tsx -- scripts/git-auto-push.ts --no-push",
    "auto:push": "tsx -- scripts/git-auto-push.ts",
    "auto:push:signoff": "tsx -- scripts/git-auto-push.ts --signoff",
    "prisma:export": "tsx -- scripts/prisma-export.ts",
    "prisma:export:compact": "tsx -- scripts/prisma-export.ts --compact",
    "prisma:import": "tsx -- scripts/prisma-import.ts",
    "check:google-config": "tsx -- scripts/check-google-config.ts",
    "lint": "eslint \"src/hooks/**/*.{ts,tsx}\" \"src/routes/**/*.{ts,tsx}\" \"src/services/**/*.{ts,tsx}\" \"src/utils/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:text-alignment": "npx tsx scripts/test-text-alignment.ts",
    "db:add:quotes": "tsx scripts/run-safe-sql.ts scripts/sql/2025-08-14_add_quotes_safe.sql",
    "db:backup": "tsx scripts/backup-db.ts",
    "db:backup:json": "tsx scripts/backup-db-json.ts",
    "db:backup:sql": "node backup-database.js",
    "db:backup:prisma": "node backup-database-prisma.js",
    "db:restore:json": "tsx scripts/restore-db-json.ts",
    "db:restore:sql": "node restore-database.js",
    "build": "npm run build:server && vite build",
    "build:server": "esbuild src/api-server-clean.ts --bundle --platform=node --format=cjs --out-extension:.js=.cjs --target=node20 --packages=external --sourcemap --outdir=dist-server",
    "build:client": "tsc -p tsconfig.build.json && vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "start": "NODE_ENV=production node dist-server/api-server-clean.cjs",
    "prod": "npm run build && npm run start",
    "test:ai": "tsx scripts/test-ai-calendar.ts",
    "test:ai:extended": "tsx scripts/test-ai-extended.ts",
    "test:ai:guard": "AI_MODE=force-live tsx scripts/test-ai-guard.ts",
    "test:kwh": "node scripts/test-kwh-condition.cjs",
    "test:kwh:matrix": "node scripts/test-kwh-condition-matrix.cjs",
    "test:kwh:auto": "node scripts/run-kwh-tests-auto.cjs",
    "dev:token": "node scripts/generate-dev-token.cjs --email=jonathan.dethier@2thier.be",
    "get:token": "node scripts/get-token.cjs --export",
    "db:create:client-form": "tsx scripts/create-client-info-form.ts",
    "db:seed:field-types": "tsx scripts/seed-field-types.ts",
    "db:map:client-dbfields": "tsx scripts/map-dbfields-client.ts",
    "devis1minute:activate": "node activate-devis1minute-modules.js",
    "devis1minute:test": "node test-devis1minute-system.js",
    "devis1minute:count": "node count-devis1minute-modules.js",
    "test:repeater": "npx tsx test-all.mjs",
    "test:variables": "npx tsx test-variables-creation.mjs",
    "test:api-display": "npx tsx test-api-display-nodes.mjs",
    "test:duplication": "npx tsx test-duplication.mjs",
    "perf:lighthouse": "npx lighthouse http://localhost:5173/devis1minute --output=json --only-categories=performance",
    "perf:lighthouse:full": "npx lighthouse http://localhost:5173/devis1minute --output=html --output-path=lighthouse-report.html",
    "perf:analyze": "npm run build:analyze && open dist/stats.html",
    "monitor:perf": "node scripts/performance-monitor.mjs",
    "analyze:triggers": "node scripts/analyze-triggers.mjs",
    "analyze:logs": "node scripts/analyze-logs.mjs",
    "benchmark": "node scripts/benchmark-guide.mjs"
  },
